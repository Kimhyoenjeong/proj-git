<script>
 
    function acyncMovePage(url){
        // ajax option
        var ajaxOption = {
                url : url,
                async : true,
                type : "POST",
                dataType : "html",
                cache : false
        };
        
        $.ajax(ajaxOption).done(function(data){
            // Contents 영역 삭제
            $('#contents-banner').children().remove();
            // Contents 영역 교체
            $(#contents-banner').html(data);
        });
    }
 
</script>
